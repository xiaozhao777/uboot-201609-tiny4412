/dts-v1/;
#include "exynos4412.dtsi"

/ {
	model = "FriendlyARM TINY4412 board based on Exynos4412";
	compatible = "friendlyarm,tiny4412", "samsung,exynos4412", "samsung exynos4" ;

  aliases {
    serial0= "/serial@13800000";
    console = "/serial@13800000";
    mmc2 = "/sdhci@12530000";
    mmc4 = "/dwmmc@12550000";
	};

  memory {
      reg = <0x40000000 0x40000000>;
    };

  serial0:serial@13800000 {
		status = "okay";
	};

  serial1:serial@13810000 {
        status = "disable";
    };

  serial2:serial@13820000 {
        status = "disable";
    };

  serial3:serial@13830000 {
        status = "disable";
    };

  serial4:serial@13840000 {
        status = "disable";
    };

  i2c@13860000 {
        status = "disable";
    };

  i2c@13870000 {
        status = "disable";
    };

  i2c@13880000 {
        status = "disable";
    };

  i2c@13890000 {
        status = "disable";
    };

  i2c@138a0000 {
        status = "disable";
    };

  i2c@138b0000 {
        status = "disable";
    };

  i2c@138c0000 {
        status = "disable";
    };

  i2c@138d0000 {
        status = "disable";
    };

  sdhci@12510000 {
        compatible = "samsung,exynos4412-sdhci";
        status = "disable";
    };

  sdhci@12520000 {
        compatible = "samsung,exynos4412-sdhci";
        status = "disable";
    };

  sdhci@12530000 {
        compatible = "samsung,exynos4412-sdhci";
        samsung,bus-width = <4>;
        samsung,timing = <1 2 3>;
        cd-gpios = <&gpk2 2 0>;
    };

  sdhci@12540000 {
        compatible = "samsung,exynos4412-sdhci";
        status = "disable";
    };

  dwmmc@12550000 {
      samsung,bus-width = <8>;
      samsung,timing = <2 1 0>;
      samsung,removable = <0>;
      fifoth_val = <0x203f0040>;
      bus_hz = <400000000>;
      div = <0x3>;
      index = <4>;
    };

  ehci@12580000 {
      compatible = "samsung,exynos-ehci";
      reg = <0x12580000 0x100>;
      #address-cells = <1>;
      #size-cells = <1>;
      phy {
          compatible = "samsung,exynos-usb-phy";
          reg = <0x125B0000 0x100>;
      };
     };

   emmc-reset {
       compatible = "samsung,emmc-reset";
       reset-gpio = <&gpk1 2 0>;
     };
};






